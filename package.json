{
  "name": "@sinoui/alert",
  "version": "1.0.0",
  "description": "警告提示，展现需要关注的信息。",
  "license": "MIT",
  "main": "dist/index.js",
  "umd:main": "dist/sinoui-alert.umd.production.js",
  "module": "dist/sinoui-alert.esm.js",
  "types": "types/index.d.ts",
  "homepage": "https://sinoui.github.io/alert",
  "bugs": {
    "url": "https://github.com/sinoui/alert/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sinoui/alert.git"
  },
  "scripts": {
    "start": "ts-lib-tools watch",
    "test": "ts-lib-tools test",
    "build": "ts-lib-tools build",
    "format": "ts-lib-tools format",
    "lint": "ts-lib-tools lint",
    "prepublishOnly": "ts-lib-tools build",
    "doc:dev": "docz dev",
    "doc:build": "docz build",
    "doc:publish": "docz build && gh-pages -d .docz/dist"
  },
  "prettier": {
    "printWidth": 80,
    "useTabs": false,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always"
  },
  "eslintConfig": {
    "extends": "ts-lib"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "cross-env CI=true ts-lib-tools test",
      "eslint --fix --color",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "files": [
    "dist",
    "assets"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@babel/runtime": "^7.7.7",
    "@sinoui/core": "^1.0.0-beta.9",
    "@sinoui/theme": "^1.0.0-beta.8",
    "@sinoui/icons": "^1.0.0-beta.2",
    "assert": "^2.0.0",
    "classnames": "^2.2.6",
    "lodash-es": "^4.17.15",
    "polished": "^3.5.2",
    "react-icons": "^3.8.0",
    "react-transition-group": "^4.3.0",
    "sinoui-components": "^0.2.9"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "styled-components": ">=4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@sinoui/ripple": "^1.0.9",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/classnames": "^2.2.9",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-transition-group": "^4.2.3",
    "@types/styled-components": "^4.4.2",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.1",
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.0.1",
    "jest-styled-components": "^7.0.2",
    "lint-staged": "^9.5.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "style-loader": "^1.1.2",
    "ts-lib-tools": "^0.8.6",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.4",
    "webpack-docz-ghpages-plugin": "^0.8.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
